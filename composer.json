{
	"name": "fuseboxy/fuseboxy-installer",
	"description": "Fuseboxy Custom Installer for Composer Installation",
	"type": "composer-plugin",
	"license": "MIT",
	"autoload": {
		"psr-4": {"Fuseboxy\\Composer\\": "src/"}
	},
	"extra": {
		"class": "Fuseboxy\\Composer\\CoreInstallerPlugin",
		"class": "Fuseboxy\\Composer\\ModuleInstallerPlugin"
	},
	"require": {
		"composer-plugin-api": "^1.0"
	},
	"require-dev": {
		"composer/composer": "^1.3"
	}
}